<UserControl
    x:Class="graphicLayer.Controles.MenuCoordinadora"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
    xmlns:local="clr-namespace:graphicLayer.Controles"
    xmlns:materialDesign="http://materialdesigninxaml.net/winfx/xaml/themes"
    xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
    d:Background="White"
    d:DesignHeight="450"
    d:DesignWidth="800"
    mc:Ignorable="d">
    <Grid>
        <Menu materialDesign:MenuAssist.TopLevelMenuItemHeight="25">
            <MenuItem
                Name="BtnAdministrarEE"
                Click="BtnAdministrarEE_Click"
                Header="Experiencias educativas" />
            <MenuItem
                Name="BtnAdministrarEstudiante"
                Header="Estudiantes" />
            <MenuItem
                Name="BtnAdministrarFechasdesesión"
                Header="Fechas de sesión de tutoria academica" />
        </Menu>
    </Grid>
</UserControl>
