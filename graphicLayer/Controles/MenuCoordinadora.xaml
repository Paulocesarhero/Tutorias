<UserControl
    x:Class="graphicLayer.Controles.MenuCoordinadora"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:materialDesign="http://materialdesigninxaml.net/winfx/xaml/themes"
    xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
    d:Background="White"
    d:DesignHeight="450"
    d:DesignWidth="800"
    mc:Ignorable="d">
    <Grid>
        <Menu materialDesign:MenuAssist.TopLevelMenuItemHeight="25">
            <MenuItem
                Name="BtnExit"
                Click="BtnExit_Click"
                Header="Salir" />
            <MenuItem
                Name="BtnAdministrarEstudiante"
                Header="Estudiantes" />
            <MenuItem
            Name="BtnExperiencias"
            Header="Experiencias" Click="BtnExperiencias_Click" />
            <MenuItem
                Name="BtnAdministrarFechasdesesión"
                Header="Fechas de sesión de tutoria academica" />
            <MenuItem
                Name="BtnAdministrarTutoresAcademicos"
                Header="Tutores academicos" Click="BtnAdministrarTutoresAcademicos_Click" />
        </Menu>
    </Grid>
</UserControl>